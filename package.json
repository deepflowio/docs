{
  "name": "theme-vdoing-blog",
  "version": "1.0.0",
  "scripts": {
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && vuepress dev docs",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && vuepress build docs --max-concurrency=500",
    "deploy": "bash deploy.sh",
    "editFm": "node utils/editFrontmatter.js",
    "baiduPush": "node utils/baiduPush.js https://xugaoyi.com && bash baiduPush.sh",
    "publish": "cd ./vdoing && npm publish && cd .. && yarn updateTheme",
    "updateTheme": "yarn remove vuepress-theme-vdoing && rm -rf node_modules && yarn && yarn add vuepress-theme-vdoing -D"
  },
  "license": "MIT",
  "devDependencies": {
    "dayjs": "^1.9.7",
    "html-to-text": "^7.0.0",
    "inquirer": "^7.1.0",
    "json2yaml": "^1.1.0",
    "mdurl": "1.0.1",
    "vuepress": "1.9.7",
    "vuepress-plugin-demo-block": "^0.7.2",
    "vuepress-plugin-one-click-copy": "^1.0.2",
    "vuepress-plugin-thirdparty-search": "^1.0.2",
    "vuepress-plugin-zooming": "^1.1.7",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "@vuepress/shared-utils": "^1.9.7",
    "axios": "^1.2.1",
    "better-scroll": "^2.4.2",
    "clipboard": "^2.0.10",
    "flexsearch": "^0.6.23",
    "good-storage": "^1.1.1",
    "gray-matter": "^4.0.3",
    "json5": "^2.2.1",
    "mermaid": "8.14.0",
    "viewerjs": "^1.11.6"
  }
}
